[user]
	name = Shota Iguchi
	email = shota-iguchi@cookpad.com
[alias]
	s = status
	d = diff
	c = commit
	co = checkout
	b = branch
	m = merge
	l = log
	a = add
	delete-all-branches = !git branch | grep -v "master" | xargs git branch -D
	delete-merged-branches = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
[color]
	ui = auto
[push]
	default = matching
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[merge "mergepbx"]
	name = Xcode project files merger
	driver = mergepbx %O %A %B

[user]
	name = Shota Iguchi
	email = s.iguchi1124@gmail.com
[alias]
	s = status
	d = diff
	c = commit
	co = checkout
	b = branch
	m = merge
	l = log
	a = add
	delete-all-branches = !git branch | grep -v \\* | grep -v 'master' | xargs git branch -D
	delete-merged-branches = !git branch --merged | grep -v \\* | grep -v 'master' | xargs git branch -d
	current = symbolic-ref --quiet --short HEAD
[color]
	ui = auto
[push]
	default = matching
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[include]
	path = ~/.gitconfig.local
